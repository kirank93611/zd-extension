{
  "manifest_version":3,
  "name":"Support Assistant",
  "version":"1.0",
  "permissions":["tabs","activeTab","storage"],
  "background":{
    "service_worker":"background.js"
  },
  "action":{
    "default_popup":"index.html",
    "default_icon":"logo512.png"
  },
  "content_scripts":[{
    "matches": ["https://*.zendesk.com/*"],
    "js": ["contentScript.js"]
  }]
}